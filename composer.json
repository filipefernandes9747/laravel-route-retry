{
    "name": "filipefernandes/laravel-route-retry",
    "description": "A Laravel package to capture and retry failing requests.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Fernandes",
            "email": "filipe9747@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelRouteRetry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelRouteRetry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRouteRetry\\RetryServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    }
}